{
    "name": "bitfertig/mustbepublished",
    "description": "MustBePublished package.",
    "keywords": [
        ":vendor_name",
        "laravel",
        ":package_slug"
    ],
    "type": "library",
    "homepage": "https://github.com/:vendor_slug/:package_slug",
    "license": "MIT",
    "authors": [
        {
            "name": "John Doe",
            "email": "john@doe.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bitfertig\\MustBePublished\\": "src"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Bitfertig\\MustBePublished\\Tests\\": "tests"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Bitfertig\\MustBePublished\\MustBePublishedServiceProvider",
                "Bitfertig\\MustBePublished\\Providers\\MacroServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.2"
    },
    "require-dev": {
        "orchestra/testbench": "6.0",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
